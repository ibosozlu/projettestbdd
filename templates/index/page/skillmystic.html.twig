{% extends 'base.html.twig' %}

{% block body %}

    <table class="table design">
        <tr class="colorblue">
            <th class="contour">ID</th>
            <th class="contour">Nom</th>
            <th class="contour">CD</th>
            <th class="contour">Icon</th>
            <th class="contour">Description</th>
            <th class="contour">Lvl 1</th>
            <th class="contour">Lvl 2</th>
            <th class="contour">Lvl 3</th>
            <th class="contour">Lvl 4</th>
            <th class="contour">Lvl 5</th>
            <th class="contour">Lvl 6</th>
            <th class="contour">Lvl 7</th>
            <th class="contour">Lvl 8</th>
            <th class="contour">Lvl 9</th>
            <th class="contour">Lvl 10</th>
        </tr>
        {% for skillmysticcode in skillmysticcode %}
            <tr>
                <td class="contour">{{ skillmysticcode.id }}</td>
                <td class="contour">{{skillmysticcode.nom }}</td>
                <td class="contour">{{skillmysticcode.cd }}</td>
                <td class="contour"><img src="{{ skillmysticcode.imagename }}" height=75 width=75 ></td>
                <td class="contour">{{ skillmysticcode.description }}</td>
                <td class="contour">{{ skillmysticcode.lvl1 }}</td>
                <td class="contour">{{ skillmysticcode.lvl2 }}</td>
                <td class="contour">{{skillmysticcode.lvl3 }}</td>
                <td class="contour">{{ skillmysticcode.lvl4 }}</td>
                <td class="contour">{{ skillmysticcode.lvl5 }}</td>
                <td class="contour">{{skillmysticcode.lvl6 }}</td>
                <td class="contour">{{ skillmysticcode.lvl7 }}</td>
                <td class="contour">{{skillmysticcode.lvl8 }}</td>
                <td class="contour">{{skillmysticcode.lvl9 }}</td>
                <td class="contour">{{ skillmysticcode.lvl10 }}</td>

            </tr>
        {% endfor %}
    </table>
{% endblock %}

